generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  address                  String  @id
  premium_finish_timestamp BigInt  @default(0)
  token                    token[]
}

model token {
  token       String   @id
  user        String
  type        String
  expires     DateTime @db.Timestamp(6)
  blacklisted Boolean
  users       users    @relation(fields: [user], references: [address], onDelete: Cascade)
}

model trending_1m {
  address            String @id
  collection         Json?    @db.Json
  floor        Decimal? @db.Decimal
  floor_change Decimal? @db.Decimal(78, 2)
  sales              BigInt?
  sales_change       Decimal? @db.Decimal(78, 2)
  average            Decimal? @db.Decimal
  average_change     Decimal? @db.Decimal(78, 2)
  volume             Decimal? @db.Decimal
  volume_change      Decimal? @db.Decimal(78, 2)
  unique_owner       Decimal? @db.Decimal(5, 2)
  deployed_ago       DateTime @db.Timestamp(6)
}

